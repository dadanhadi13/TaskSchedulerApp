<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kelompok5.taskschedulerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/Theme.TaskSchedulerApp">
        <activity android:name=".Hal_Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NavBottom" />
        <activity android:name=".Hal_Register" />
        <activity android:name=".Hal_EditProfil"/>
        <activity android:name=".Hal_ChangePassword"/>
        <activity android:name=".Hal_add"
            android:theme="@style/Theme.PopUp"/>
        <receiver android:name=".NotifyTask"/>
    </application>

</manifest>